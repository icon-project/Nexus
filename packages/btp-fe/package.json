{
  "name": "btp-fe",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build:testnet": "node scripts/build.js",
    "build:mainnet": "node -r dotenv/config scripts/build.js dotenv_config_path=.env.mainnet",
    "test": "node scripts/test.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "analyze": "yarn build:testnet && source-map-explorer 'build/static/js/*.js'",
    "lang": "NODE_ENV=development babel -f .babelrc 'src/**/*.{js,jsx}'",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint --ext .jsx,.js --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "prettify": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": [
      "prettier --write --ignore-path .gitignore"
    ],
    "*.{js,jsx}": [
      "yarn lint:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://git.baikal.io/web/admin-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      ".+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      ".+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web",
      ".+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "dependencies": {
    "@rematch/core": "2.0.0-next.10",
    "@rematch/loading": "2.0.0-next.11",
    "@rematch/select": "3.0.0-next.10",
    "antd": "4.10.2",
    "bfj": "7.0.2",
    "connected-react-router": "6.8.0",
    "dayjs": "1.10.4",
    "ethers": "5.4.4",
    "final-form": "4.20.2",
    "i18next": "19.8.4",
    "i18next-browser-languagedetector": "6.0.1",
    "i18next-http-backend": "1.0.23",
    "icon-sdk-js": "0.0.19",
    "identity-obj-proxy": "3.0.0",
    "qs": "6.9.6",
    "react": "17.0.1",
    "react-app-polyfill": "2.0.0",
    "react-copy-to-clipboard": "5.0.3",
    "react-dev-utils": "11.0.1",
    "react-dom": "17.0.1",
    "react-final-form": "6.5.3",
    "react-helmet-async": "1.0.7",
    "react-i18next": "11.8.5",
    "react-redux": "7.2.2",
    "react-refresh": "0.8.3",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "resolve": "1.18.1",
    "source-map-explorer": "2.5.2",
    "style-loader": "1.3.0",
    "styled-components": "5.2.1",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.3",
    "@babel/parser": "7.12.11",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.2",
    "@storybook/addon-actions": "6.1.17",
    "@storybook/addon-essentials": "6.1.11",
    "@storybook/addon-links": "6.1.11",
    "@storybook/react": "6.1.11",
    "@svgr/webpack": "5.4.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "axios-mock-adapter": "1.19.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.0",
    "babel-loader": "8.1.0",
    "babel-plugin-i18next-extract": "0.8.2",
    "babel-plugin-named-asset-import": "0.3.7",
    "babel-preset-react-app": "10.0.0",
    "camelcase": "6.1.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "4.3.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.9.2",
    "eslint-webpack-plugin": "2.1.0",
    "file-loader": "6.1.1",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "jest-circus": "26.6.0",
    "jest-resolve": "26.6.0",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "11.2.6",
    "mini-css-extract-plugin": "0.11.3",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "plop": "2.7.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "prettier": "2.2.1",
    "prompts": "2.4.0",
    "resolve-url-loader": "3.1.2",
    "sass-loader": "8.0.2",
    "semver": "7.3.2",
    "terser-webpack-plugin": "4.2.3",
    "ts-pnp": "1.2.0",
    "url-loader": "4.1.1",
    "web-vitals": "0.2.4",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.4"
  }
}
