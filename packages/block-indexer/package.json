{
  "name": "block-indexer",
  "version": "0.2.3",
  "description": "Indexing data from blockchain to local database",
  "main": "index.js",
  "author": "Tien Do",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --no-eslintrc ./src",
    "start:icon": "LOGGER_NAME=icon nodemon index.js icon",
    "start:moonbeam": "LOGGER_NAME=moonbeam nodemon index.js moonbeam",
    "start:bsc": "LOGGER_NAME=bsc nodemon index.js bsc",
    "start:near": "LOGGER_NAME=near nodemon index.js near",
    "start:harmony": "LOGGER_NAME=harmony nodemon index.js harmony",
    "pm2:icon": "pm2 start pm2.icon.json",
    "pm2:moonbeam": "pm2 start pm2.moonbeam.json",
    "pm2:bsc": "pm2 start pm2.bsc.json",
    "pm2:harmony": "pm2 start pm2.harmony.json",
    "pm2:near": "pm2 start pm2.near.json",
    "test": "jest --config=jest.json --passWithNoTests --verbose"
  },
  "dependencies": {
    "@hapi/boom": "9.1.2",
    "@hapi/joi": "17.1.1",
    "@openzeppelin/contracts": "4.2.0",
    "@slack/web-api": "6.7.1",
    "axios": "0.21.1",
    "data-fns": "0.1.5",
    "debug": "4.3.1",
    "dotenv-expand": "8.0.3",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "icon-sdk-js": "1.1.2",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "ms": "2.1.3",
    "nanoid": "3.1.23",
    "near-api-js": "0.43.1",
    "node-cron": "3.0.0",
    "pg": "8.6.0",
    "rlp": "2.2.6",
    "uuid": "8.3.2",
    "web3": "web3@3.0.0-rc.5",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "27.0.1",
    "jest-environment-node": "27.0.1",
    "nodemon": "2.0.7"
  }
}
